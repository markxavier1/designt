@model IEnumerable<protbl> 
@{
    ViewBag.Title = "ViewPro";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<div id="page-wrapper">

    <div class="row">
        <!--  page header -->
        <div class="col-lg-12">
            <h1 class="page-header">Tables</h1>
        </div>
        <!-- end  page header -->
    </div>
    <div class="row">
        <div class="col-lg-12">
            <!-- Advanced Tables -->
            <div class="panel panel-default">
                <div class="panel-heading">
                    Advanced Tables
                </div>
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>Name</th>
                                    <th>Type</th>
                                    <th>Image</th>
                                    <th>Description</th>
                                    <th></th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (protbl st in Model)
                                {
                                <tr class="gradeA">


                                    <td>@st.ProId</td>
                                    <td>@st.ProName</td>
                                    <td>@st.protType</td>
                                    <td class="center"><img src="@st.ProImg" style="width:150px" class="img-responsive" /></td>
                                    <td>@st.ProDes</td>
                                    <td><a href="@Url.Action("UpdatePro","Admin",new { id=st.ProId})" class="btn btn-default"><i class="glyphicon glyphicon-edit"></i> Edit</a></td>
                                    <td><a href="@Url.Action("DeletePro","Admin",new { id=st.ProId})" class="btn btn-default"><i class="glyphicon glyphicon-remove"></i> Delete</a></td>
                                    <td><a href="@Url.Action("AddHome","Admin",new { id=st.ProId})" class="btn btn-default"><i class="glyphicon glyphicon-remove"></i> Add At Home</a></td>
                                </tr>
                                }
                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
            <!--End Advanced Tables -->
        </div>
    </div>

</div>

